datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder", "views"]
}

generator markdown {
  provider = "node ./lib/executable/markdown"
  title    = "Virtual Drive System"
  output   = "./drive/drive.md"
}
